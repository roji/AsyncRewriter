{
  "version": "0.8.7",
  "packOptions": {
    "owners": [ "Shay Rojansky" ],
    "authors": [ "Shay Rojansky" ],
    "description": "Uses Roslyn to rewrite synchronous C# methods into async counterparts",
    "packageType": "DotnetCliTool",
    "repository": {
      "type": "git",
      "url": "git://github.com/roji/AsyncRewriter"
    }
  },
  "buildOptions": {
    "emitEntryPoint": true
  },
  "dependencies": {
    "Microsoft.Extensions.FileSystemGlobbing": "1.0.0",
    "AsyncRewriter": "0.8.7"
  },
  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.Data": "4.0.0.0",
        "System.Runtime": "4.0.10.0",
        "System.Threading.Tasks": "4.0.0.0"
      }
    },
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.0",
          "type": "platform"
        },
        "System.Console": "4.0.0",
        "System.Runtime": "4.1.0"
      }
    }
  }
}
